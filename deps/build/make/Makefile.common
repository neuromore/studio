                     -DQT_USE_QSTRINGBUILDER \
                     -DQT_NO_DEBUG \
                     -DHAVE_CONFIG_H \
                     -DUNICODE \
                     -D_ENABLE_EXTENDED_ALIGNED_STORAGE
QTXML_DEFINES_X86  = $(QTXML_DEFINES) $(QTXML_DEFINES_X86_PLAT)
QTXML_DEFINES_X64  = $(QTXML_DEFINES) $(QTXML_DEFINES_X64_PLAT)
